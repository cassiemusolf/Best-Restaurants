@{
  Layout="Shared/_Layout.cshtml";
}
<div class="jumbotron">
<h1>All Cuisines</h1>
</div>
@if (@Model.Count == 0)
{
  <p>There are no cuisines listed.</p>
}
<ul>
  @foreach (var cuisine in Model)
  {
    <li><a href="/cuisines/@cuisine.GetId()">@cuisine.GetName()</a> | <a href="/cuisine/edit/@cuisine.GetId()">Edit</a> | <a href="/cuisine/delete/@cuisine.GetId()">Delete</a></li>
  }
</ul>


<form action="/cuisines/delete" method="post">
  <button class="btn btn-lg btn-primary" type="submit">Clear All</button>
</form>

<p><a href="/cuisines/new">Add New</a> | <a href="/">Return Home</a></p>
